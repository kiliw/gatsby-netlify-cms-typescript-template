{
  "name": "gatsby-netlify-cms-typescript-template",
  "description": "Gatsby TypeScript starter",
  "version": "1.0.0",
  "author": "Kilian Wimmer <kilocheat@gmail.com>",
  "keywords": [
    "gatsby, typescript"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "start": "yarn run develop",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "lint": "concurrently \"yarn lint:code\"",
    "lint:code": "eslint 'src/**/*.{jsx,js,ts,tsx}'",
    "format": "prettier --parser typescript --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.ts*\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@types/node": "^8.0.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "0.14.23",
    "concurrently": "^5.0.0",
    "gatsby": "^2.24.32",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-netlify-cms": "^4.8.0",
    "gatsby-plugin-react-helmet": "^3.8.0",
    "gatsby-plugin-typescript": "^2.1.15",
    "gatsby-remark-component": "^1.1.3",
    "gatsby-source-filesystem": "^2.10.0",
    "gatsby-transformer-json": "^2.10.0",
    "gatsby-transformer-remark": "^2.15.0",
    "netlify-cms-app": "^2.14.12",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^5.0.1",
    "git-repo-info": "^2.1.1",
    "prettier": "^2.0.5",
    "typescript": "^4.1.3"
  }
}
